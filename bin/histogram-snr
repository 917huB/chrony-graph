#!/usr/bin/perl

use strict;
use Getopt::Long;

my($cdf);
GetOptions ("cdf" => \$cdf)
or die("Error in command line arguments\n");


my(%counts);
while(<>) {
  chomp($_);
  s/^\d+ //;
  my(@line) = split(/, /);
  for(my $i = 0; $i < @line; $i++) {
    if($line[$i] =~ /^(\d*):(\d+)$/) {
      my($snr,$count) = ($1,$2);
      if($snr ne "") {
        $counts{$snr} += $count;
      } else {
        $counts{-1} += $count;
      }
    } else {
      print STDERR "$0 unknown part: $_\n";
    }
  }
}

if($cdf) {
  my($sum) = 0;
  foreach my $count (sort { $a <=> $b } keys %counts) {
    $sum += $counts{$count};
    print $count," $sum\n";
  }
} else {
  foreach my $count (sort { $a <=> $b } keys %counts) {
    print $count," ",$counts{$count},"\n";
  }
}
